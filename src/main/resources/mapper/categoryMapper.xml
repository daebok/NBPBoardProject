<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hyunhye.board.categoryMapper">

	<resultMap id="list" type="com.hyunhye.board.model.CategoryModel">
		<result property="categoryId" column="categoryId" javaType="java.lang.Integer" />
		<result property="item" column="item" javaType="java.lang.String" />
	</resultMap>

	<select id="categoryList" resultMap="list">
		SELECT item
		FROM CATEGORY
	</select>
	
	<select id="select" resultMap="list">
		SELECT categoryId
		FROM CATEGORY
		WHERE item = #{item}
	</select>
</mapper>
