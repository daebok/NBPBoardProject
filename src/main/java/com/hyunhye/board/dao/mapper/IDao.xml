<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hyunhye.board.dao.IDao">
	<select id="listDao" resultType="com.hyunhye.board.dto.ContentDto">
		SELECT * FROM USER
	</select>
	
	
	<insert id="writeDao">
		INSERT INTO USER (ID, PASSWORD, NAME) VALUES (#{param1}, #{param2}, #{param3})
	</insert>
	
	
	<delete id="deleteDao">
		DELETE FROM USER WHERE MID = #{param1}
	</delete>
	
</mapper>
